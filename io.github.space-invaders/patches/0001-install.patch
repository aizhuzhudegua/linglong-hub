 create mode 100644 space.desktop

diff --git a/AlienInvaders.pro b/AlienInvaders.pro
index 1653a6c..f34d472 100644
--- a/AlienInvaders.pro
+++ b/AlienInvaders.pro
@@ -47,3 +47,17 @@ HEADERS  += \
 
 RESOURCES += \
     res.qrc
+
+
+unix: {
+
+target.path = $${PREFIX}/bin
+INSTALLS += target
+
+unix_desktop.path = $${PREFIX}/share/applications
+unix_desktop.files = space.desktop
+INSTALLS += unix_desktop
+
+
+}
+
diff --git a/space.desktop b/space.desktop
new file mode 100644
index 0000000..abb9c78
--- /dev/null
+++ b/space.desktop
@@ -0,0 +1,7 @@
+[Desktop Entry]
+Exec=AlienInvaders
+GenericName=AlienInvaders
+Hidden=false
+Name=AlienInvaders
+StartupNotify=false
+Type=Application
\ No newline at end of file
